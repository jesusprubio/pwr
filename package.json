    "gitignore": "0.6.0",
    "http-server": "^0.11.1",
    "license": "0.0.3",
    "internal-ip-cli": "^1.0.0",
    "is-online-cli": "^4.0.0",
    "package-json-cli": "2.0.0",
    "np": "^5.1.1",
    "npm-check": "^5.9.0",
    "npm-consider": "^1.7.0",
    "npm-name-cli": "^2.6.1",
    "ntl": "^4.0.0",
    "open-editor-cli": "^1.0.0",
    "prompts": "^2.2.1",
    "public-ip-cli": "^2.0.0",
    "puppeteer-cli": "^1.4.0",
    "share-cli": "^3.0.0",
    "terminal-link": "^2.0.0",
    "trevor": "^2.4.0",
    "vtop": "^0.6.1",
    "yo": "^3.1.0"
  },
  "devDependencies": {
    "audit-ci": "^2.4.1",
    "depcheck": "^0.8.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "jest"
    ],
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "strict": [
        "off"
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "testEnvironment": "node"
  }
}

    "pretest": "npm run audit && npm run deps && npm run lint",
    "test": "jest",
    "format": "prettier --write '**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "await-spawn": "^3.0.0",
    "chalk": "^2.4.2",
    "copy-github-labels-cli": "^1.0.0",

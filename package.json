{
  "name": "pwr",
  "version": "1.2.1",
  "description": "âš¡ The (cheated) interactive CLI for npm",
  "keywords": [
    "cli",
    "code quality",
    "convenience",
    "productivity",
    "security",
    "tool",
    "workflow"
  ],
  "homepage": "https://github.com/jesusprubio/pwr",
  "bugs": {
    "url": "https://github.com/jesusprubio/pwr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jesusprubio/pwr.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jesus Rubio",
    "url": "http://jesusprubio.dev"
  },
  "files": [
    "bin",
    "scripts",
    "tools.json"
  ],
  "bin": {
    "pwr": "./bin/index.js"
  },
  "scripts": {
    "audit": "audit-ci",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "start": "node bin",
    "pretest": "npm run audit",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "strict": [
        "error",
        "safe"
      ],
      "no-param-reassign": [
        "off"
      ]
    },
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "await-spawn": "^3.0.0",
    "chalk": "^3.0.0",
    "esc-exit": "^2.0.2",
    "global-dirs": "^2.0.1",
    "import-from": "^3.0.0",
    "inquirer": "^7.0.0",
    "p-each-series": "^2.1.0",
    "terminal-link": "^2.0.0"
  },
  "devDependencies": {
    "audit-ci": "^2.4.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1"
  },
  "peerDependencies": {
    "copy-github-labels-cli": "^1.0.0",
    "del-nm-cli": "^2.2.3",
    "emma-cli": "^2.1.0",
    "fast-cli": "^2.3.0",
    "fkill-cli": "^5.2.0",
    "gitignore": "^0.6.0",
    "http-server": "^0.11.1",
    "internal-ip-cli": "^1.0.0",
    "is-online-cli": "^4.0.0",
    "license": "^0.0.3",
    "np": "^5.1.3",
    "npm-check": "^5.9.0",
    "npm-consider": "^1.7.0",
    "npm-name-cli": "^2.6.1",
    "ntl": "^4.0.0",
    "open-editor-cli": "^1.0.0",
    "package-json-cli": "2.0.0",
    "public-ip-cli": "^2.0.0",
    "puppeteer-cli": "^1.4.0",
    "share-cli": "^3.0.0",
    "trevor": "^2.4.0",
    "vtop": "^0.6.1",
    "yo": "^3.1.0"
  },
  "engines": {
    "node": ">=10"
  }
}

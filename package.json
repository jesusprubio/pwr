  "keywords": [
    "CLI",
    "tool",
    "workflow",
    "security",
    "code quality",
    "productivity",
    "convenience"
  ],
  "homepage": "https://github.com/jesusprubio/pwr",
  "bugs": {
    "url": "https://github.com/jesusprubio/pwr/issues"

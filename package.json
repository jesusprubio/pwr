    "lint": "eslint .",
    "pretest": "npm run audit && npm run deps && npm run lint",
    "test": "nyc ava",
    "format": "prettier --write '**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "await-spawn": "^2.1.2",
    "chalk": "^2.4.2",
    "copy-github-labels-cli": "^1.0.0",
    "del-nm-cli": "^2.2.3",
    "emma-cli": "^2.1.0",
    "esc-exit": "^2.0.2",
    "fast-cli": "^2.3.0",
    "fkill-cli": "^5.2.0",
    "http-server": "^0.11.1",
    "license": "0.0.3",
    "internal-ip-cli": "^1.0.0",
    "is-online-cli": "^4.0.0",
    "np": "^5.0.2",
    "npm-check": "^5.9.0",
    "npm-consider": "^1.7.0",
    "npm-name-cli": "^2.5.0",
    "ntl": "^3.2.4",
    "nyc": "^14.1.1",
    "open-editor-cli": "^1.0.0",
    "prompts": "^2.1.0",
    "public-ip-cli": "^2.0.0",
    "puppeteer-cli": "^1.3.2",
    "share-cli": "^3.0.0",
    "terminal-link": "^1.3.0",
    "trevor": "^2.4.0",
    "vtop": "^0.6.1",
    "yo": "^2.0.6"
  },
  "devDependencies": {
    "audit-ci": "^1.7.0",
    "ava": "^1.4.1",
    "depcheck": "^0.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "strict": [
        "off"
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
